<h1>Usuarios</h1>
<p>Lista de usuarios iFreelance</p>

<table class="table">
    <thead>
        <tr>
        <th>Acciones</th>
        <th>Email</th>
        <th>Rol</th>
        <th>Fecha Creación</th>
        <th>Ultima actualización</th>
        <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
          <tr>
          <td></td>
          <td><%= user.email %></td>
          <td><%= user.role %></td>
          <td><%= user.created_at %></td>
          <td><%= user.updated_at %></td>
          <td><%= link_to 'Ver', user %></td>
          <td><%= link_to 'Editar', edit_user_path(user) %></td>
          </tr>
        <% end %>
    </tbody>
</table>
