<p id="notice"><%= notice %></p>

<h1>Invoices</h1>

<table>
  <thead>
    <tr>
      <th>Design view</th>
      <th>Programming front</th>
      <th>Design date base</th>
      <th>Programming back</th>
      <th>Banner</th>
      <th>Advertising</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoices.each do |invoice| %>
      <tr>
        <td><%= invoice.design_view %></td>
        <td><%= invoice.programming_front %></td>
        <td><%= invoice.design_date_base %></td>
        <td><%= invoice.programming_back %></td>
        <td><%= invoice.banner %></td>
        <td><%= invoice.advertising %></td>
        <td><%= link_to 'Show', invoice %></td>
        <td><%= link_to 'Edit', edit_invoice_path(invoice) %></td>
        <td><%= link_to 'Destroy', invoice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Invoice', new_invoice_path %>
